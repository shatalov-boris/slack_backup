- @opened_channels.each do |type, channels|
  .row
    .col-xs-12
      %h4= type.humanize
      %table.table.table-striped
        - channels.each do |channel|
          %tr
            %td= link_to channel_name(channel), channel_path(channel)

.row
  .col-xs-12
    %h4 Archived Channels
    %table.table.table-striped
      - @archived_channels.each do |channel|
        %tr
          %td= link_to channel_name(channel), channel_path(channel)
